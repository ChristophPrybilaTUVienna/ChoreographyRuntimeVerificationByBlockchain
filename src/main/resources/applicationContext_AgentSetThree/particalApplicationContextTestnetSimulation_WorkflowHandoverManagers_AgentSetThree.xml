<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Bitcoin workflow storages for all simulated companies -->
    <bean id="handoverManager_I"
          class="at.ac.tuwien.infosys.prybila.runtimeVerification.handoverFramework.WorkflowHandoverManager">
        <constructor-arg index="0" value="org.bitcoin.test"/>
        <constructor-arg index="1" value="./testfiles/simulation/svp_testnet_company_I.store"/>
        <constructor-arg index="2" value="./testfiles/simulation/svp_testnet_company_I.wallet"/>
        <constructor-arg index="3">
            <null/>
        </constructor-arg>
        <constructor-arg index="4" value="./testfiles/simulation/svp_testnet_company_I.handoverStore"/>
        <constructor-arg index="5" value="false"/>
        <property name="ownIdentityProvider" ref="ownIdentityProvider_I"/>
    </bean>

    <bean id="handoverManager_J"
          class="at.ac.tuwien.infosys.prybila.runtimeVerification.handoverFramework.WorkflowHandoverManager">
        <constructor-arg index="0" value="org.bitcoin.test"/>
        <constructor-arg index="1" value="./testfiles/simulation/svp_testnet_company_J.store"/>
        <constructor-arg index="2" value="./testfiles/simulation/svp_testnet_company_J.wallet"/>
        <constructor-arg index="3">
            <null/>
        </constructor-arg>
        <constructor-arg index="4" value="./testfiles/simulation/svp_testnet_company_J.handoverStore"/>
        <constructor-arg index="5" value="false"/>
        <property name="ownIdentityProvider" ref="ownIdentityProvider_J"/>
    </bean>

    <bean id="handoverManager_K"
          class="at.ac.tuwien.infosys.prybila.runtimeVerification.handoverFramework.WorkflowHandoverManager">
        <constructor-arg index="0" value="org.bitcoin.test"/>
        <constructor-arg index="1" value="./testfiles/simulation/svp_testnet_company_K.store"/>
        <constructor-arg index="2" value="./testfiles/simulation/svp_testnet_company_K.wallet"/>
        <constructor-arg index="3">
            <null/>
        </constructor-arg>
        <constructor-arg index="4" value="./testfiles/simulation/svp_testnet_company_K.handoverStore"/>
        <constructor-arg index="5" value="false"/>
        <property name="ownIdentityProvider" ref="ownIdentityProvider_K"/>
    </bean>

    <bean id="handoverManager_L"
          class="at.ac.tuwien.infosys.prybila.runtimeVerification.handoverFramework.WorkflowHandoverManager">
        <constructor-arg index="0" value="org.bitcoin.test"/>
        <constructor-arg index="1" value="./testfiles/simulation/svp_testnet_company_L.store"/>
        <constructor-arg index="2" value="./testfiles/simulation/svp_testnet_company_L.wallet"/>
        <constructor-arg index="3">
            <null/>
        </constructor-arg>
        <constructor-arg index="4" value="./testfiles/simulation/svp_testnet_company_L.handoverStore"/>
        <constructor-arg index="5" value="false"/>
        <property name="ownIdentityProvider" ref="ownIdentityProvider_L"/>
    </bean>

    <import resource="particalApplicationContextSimulation_Identification_AgentSetThree.xml"/>

</beans>