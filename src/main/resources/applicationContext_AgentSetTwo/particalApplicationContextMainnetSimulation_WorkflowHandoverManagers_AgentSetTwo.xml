        <constructor-arg index="2" value="./testfiles/simulation/svp_mainnet_company_G.wallet"/>
        <constructor-arg index="3" value="./src/main/resources/mainnet_checkpoints_2016-05-14T12.54.27Z"/>
        <constructor-arg index="4" value="./testfiles/simulation/svp_mainnet_company_G.handoverStore"/>
        <constructor-arg index="5" value="false"/>
